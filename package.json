{
	"name": "ts-meo-discord",
	"version": "0.2.0",
	"private": true,
	"module": "src/index.ts",
	"type": "module",
	"scripts": {
		"start:dev": "NODE_ENV=development bun src/index.ts",
		"command:dev": "NODE_ENV=development bun src/deploy-commands.ts",
		"start:prod": "NODE_ENV=production bun src/index.ts",
		"command:prod": "NODE_ENV=production bun src/deploy-commands.ts",
		"format": "biome format ./src/",
		"lint": "biome lint ./src/",
		"check": "biome check ./src/",
		"fix:format": "biome format --write ./src/",
		"fix:lint": "biome lint --write ./src/",
		"fix:check": "biome check --write ./src/"
	},
	"dependencies": {
		"axios": "^1.7.5",
		"discord.js": "^14.15.3",
		"twitter-api-v2": "^1.17.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"lefthook": "^1.7.14",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.5.4"
	}
}
