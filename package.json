{
	"name": "ts-meo-discord",
	"version": "0.2.0",
	"private": true,
	"module": "src/index.ts",
	"type": "module",
	"scripts": {
		"start:dev": "NODE_ENV=development bun src/index.ts",
		"format": "biome format ./src/",
		"lint": "biome lint ./src/",
		"check": "biome check ./src/",
		"fix:format": "biome format --write ./src/",
		"fix:lint": "biome lint --write ./src/",
		"fix:check": "biome check --write ./src/",
		"start:prod": "NODE_ENV=production bun src/index.ts"
	},
	"dependencies": {
		"axios": "1.7.7",
		"discord.js": "14.16.3",
		"twitter-api-v2": "^1.18.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"lefthook": "1.8.1",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "5.6.3"
	}
}
