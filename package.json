{
	"name": "discord-bot",
	"version": "1.0.9",
	"private": true,
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "bun --hot src/index.ts",
		"deploy-commands": "bun scripts/deploy-commands.ts",
		"check": "biome check ./src/",
		"check:fix": "biome check --write ./src/",
		"typecheck": "tsc --noEmit",
		"changeset": "changeset",
		"version": "changeset version && bun i --lockfile-only"
	},
	"dependencies": {
		"discord.js": "14.25.0",
		"twitter-api-v2": "1.28.0",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.6",
		"@changesets/changelog-github": "0.5.1",
		"@changesets/cli": "2.29.7",
		"@types/bun": "1.3.5",
		"lefthook": "2.0.4"
	},
	"peerDependencies": {
		"typescript": "5.9.3"
	},
	"engines": {
		"bun": ">=1.3.2"
	},
	"packageManager": "bun@1.3.2"
}
