{
	"name": "discord-bot",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "bun --hot src/index.ts",
		"deploy-commands": "bun scripts/deploy-commands.ts",
		"check": "biome check ./src/",
		"check:fix": "biome check --write ./src/",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"discord.js": "14.24.2",
		"neverthrow": "8.2.0",
		"twitter-api-v2": "1.28.0",
		"winston": "3.18.3",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"lefthook": "2.0.4",
		"@types/bun": "1.3.2"
	},
	"peerDependencies": {
		"typescript": "5.9.3"
	},
	"engines": {
		"bun": ">=1.3.2"
	},
	"packageManager": "bun@1.3.2"
}
